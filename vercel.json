{
  "version": 2,
  "builds": [
    {
      "src": "web/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web/app.py"
    }
  ],
  "functions": {
    "web/app.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
